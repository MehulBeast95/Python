#####################################
a=[]
def push(a,val):
    a.append(val)
    print("Element pushed succesfully")
def pop_item(a):
    val=a.pop()
    print(val)
def peek(a):
    index=len(a)-1
    print("Peek element: ",a[index])
def display(a):
    for i in range(len(a)-1,-1,-1):
        print(a[i])
while True:
    print("1->Push \n2->Pop \n3->Peek \n4->Display \n5->Exit")
    ch=int(input("Choose option: "))
    if ch==1:
        val=int(input("Enter number to push: "))
        push(a,val)
    elif ch==2:
        if len(a)==0:
            print("Empty stack")
        else:
            pop_item(a)
    elif ch==3:
        if len(a)==0:
            print("Empty stack")
        else:
            peek(a)
    elif ch==4:
        if len(a)==0:
            print("Empty stack")
        else:
            display(a)
    else:
        break
