d={"Tanvi":76000,"Mehul":90000,"Anil":56000,"Manvi":43000,"Joe":9000}
a=[]
def push(name):
    a.append(name)
def pop():
    if a==[]:
        return None
    else:
        return a.pop()
for key in d:
    if d[key]>45000:
        push(key)
while True:
    na=pop()
    if na!=None:
        print(na,end='\t')
    else:
        break
