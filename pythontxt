        sql = "select * from account where Adharno=%s"
        cur.execute(sql, rl)
    elif ch == 5:
        sql = "select * from account"
        cur.execute(sql)


recs = cur.fetchall()
print("The Customer details are as follows: ")
k = pd.DataFrame(recs, columns=['AccNo', 'Name', 'Age', 'Occup', 'Add', 'Mob', 'Adharno', 'Amount', 'Acctype'])
