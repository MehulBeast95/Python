stk=[]
def push():
    while True:
        x=input("Enter name: ")
        stk.append(x)
        display()
        ch=input("Enter more ? :  ")
        if ch=="no":
            break
def pop():
    if stk==[]:
        print("Underflow")
    else:
        z=stk.pop()
        print("deleted element: ",z)
        display()
def display():
    a=len(stk-1)
    for i in range(a-1,-1,-1):
                   print(stk[i])
while True:
    print("1.Add\n2.Delete \n3.Display \n4.Exit \n")
    c=int(input("Enter choice(1-4): "))
    if c==1:
        push()
    elif c==2:
        pop()
    elif c==3:
        display()
    else:
        break
###############################################
x=str(input("Enter string:"))
while True:
    print("what do you wanna do?")
    print("1. Check palindrome")
    print("2. Swap case of string")
    print("3. Exit")
    n=int(input("choose:"))
    if n==1:
        def ispalin(str):
            for i in range(0, int(len(str)/2)):
                if str[i] != str[len(str)-i-1]:
                    return False
            return True
        y=ispalin(x)
        if (y):
            print(x,"is a palindrome")
        else:
            print(x,"is not a palindrome")
    elif n==2:
        print(x.swapcase())

    else:
        break
