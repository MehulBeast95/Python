with open("sample1.txt","r") as f:
    d={}
    x=f.read()
    word=x.split()
    for i in word:
        if i in d.keys():
            d[i]+=1
        else:
            d[i]=1
    m=max(d.values())
    for i in d:
        if d[i]==m:
            print(i,":",d[i])
