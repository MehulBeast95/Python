while c == 'yes':
        if c == 'no':
            break
        ch = (input("Enter Character: "))
        if 65 <= ord(ch) <= 90:
            print('Capital Letters')
        elif 97 <= ord(ch) <= 122:
            print('Small Letters')
        elif 48 <= ord(ch) <= 57:
            print('Digits')
        else:
            print('Special Character')
        c = input('Want to Continue? ')
        if c != 'yes' or 'no':
            print("I don't understand that!!!")
