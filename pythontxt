f=open("sample1.txt","r")
str=f.read()
a=0
b=0
c=0
d=0
for i in str:
    if (i.isupper()):
        c=c+1
    elif (i.islower()):
        d=d+1
    i=i.lower()
    if i in "aeiouAEIOU":
        a=a+1
    elif i in "bcdfghjklmnpqrstvwxyzBCDFGHJKLMNPQRSTVWXYZ":
        b=b+1
    else:
        pass
print("no of vowels: ",a)
print("no of consonants: ",b)
print("no of uppercase: ",c)
print("no of lowercase: ",d)
f.close()
