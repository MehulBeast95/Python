n=int(input("Enter no. of elements: "))
l=[]
for i in range(n):
    a=int(input("Enter a number: "))
    l.append(a)
print("Given list: ",l)
for i in range(1,n,2):
    l[i],l[i-1]=l[i-1],l[i]
print("List after swapping: ",l)
