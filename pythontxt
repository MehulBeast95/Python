def new_game():
    g= []
    c_g = 0
    x = 1
    for key in d:
        y=random.choice(l1)
        print("-------------------------")
        print(key)
        for i in l[x-1]:
            print(i)
        a = input("Enter (1,2,3 or 4): ")
        a = a.upper()
        g.append(a)
        c_g += check_answer(d.get(key), a)
        x += 1
    display_score(c_g, a)
def check_answer(answer, a):
    if answer == a:
        print("CORRECT!")
        return 1
    else:
        print("WRONG!")
        return 0
def display_score(c_g, g):
    print("-------------------------")
    print("RESULTS")
    print("-------------------------")
    score = int((c_g/len(d))*100)
    print("Your score is: "+str(score)+"%")
new_game()

























